<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>SharpSvn</id>
    <title>SharpSvn - Subversion for .Net</title>
    <version>>1.14000.12</version>
    <authors>SharpSvn</authors>
    <owners>SharpSvn</owners>
    <license type="expression">Apache-2.0</license>
    <projectUrl>https://github.com/AmpScm/SharpSvn/</projectUrl>
    <iconUrl>https://subversion.apache.org/favicon.ico</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>SharpSvn is a binding of the Subversion Client API for .NET based on the actual Subversion code, which is included in this library.</description>
    <copyright>(C) Copyright 2006-2022 The SharpSvn Project</copyright>
    <tags>Subversion Svn SharpSvn</tags>
    <repository type="git" url="https://github.com/AmpScm/SharpSvn.git" branch="main" commit="" />
    <dependencies>
      <group targetFramework="net40">
      </group>
      <group targetFramework="netcoreapp">
      </group>
    </dependencies>
    <references>
      <group targetFramework="net40">
        <reference file="SharpSvn.dll" />
        <reference file="SharpSvn.xml" />
      </group>
      <group targetFramework="netcoreapp">
        <reference file="SharpSvn.dll" />
        <reference file="SharpSvn.xml" />
      </group>
    </references>
  </metadata>
  <files>
    <!-- For legacy NuGet -->
    <file target="lib/net40" src="../src/SharpSvn/bin/win32/release/SharpSvn.dll" />
    <file target="lib/net40" src="../src/SharpSvn/bin/win32/release/SharpSvn.xml" />
    <file target="lib/net40" src="../src/SharpSvn/bin/win32/release/SharpPlink-Win32.svnExe" />
    <file target="lib/net40" src="../imports/release/bin/SharpSvn-DB44-20-win32.svnDll" />
    <!-- /legacy -->

    <!-- For PackageReference NuGet on .Net 4.x -->
    <!--  For compile time -->
    <file target="ref/net40" src="obj/SharpSvn.dll" />
    <file target="ref/net40" src="../src/SharpSvn/bin/win32/release/SharpSvn.xml" />

    <!-- For runtime -->
    <file target="runtimes/win-x86/lib/net40" src="../src/SharpSvn/bin/win32/release/SharpSvn.dll" />
    <file target="runtimes/win-x86/lib/net40" src="../src/SharpSvn/bin/win32/release/SharpSvn.pdb" />
    <file target="runtimes/win-x86/lib/net40" src="../src/SharpSvn/bin/win32/release/SharpSvn.xml" />
    <file target="runtimes/win-x86/lib/net40" src="../src/SharpSvn/bin/win32/release/SharpPlink-Win32.svnExe" />
    <file target="runtimes/win-x86/lib/net40" src="../imports/release/bin/SharpSvn-DB44-20-win32.svnDll" />

    <file target="runtimes/win-x64/lib/net40" src="../src/SharpSvn/bin/x64/release/SharpSvn.dll" />
    <file target="runtimes/win-x64/lib/net40" src="../src/SharpSvn/bin/x64/release/SharpSvn.pdb" />
    <file target="runtimes/win-x64/lib/net40" src="../src/SharpSvn/bin/x64/release/SharpSvn.xml" />
    <file target="runtimes/win-x64/lib/net40" src="../src/SharpSvn/bin/x64/release/SharpPlink-x64.svnExe" />
    <file target="runtimes/win-x64/lib/net40" src="../imports/release/bin/SharpSvn-DB44-20-x64.svnDll" />
    <!-- /PackageReference -->

    <!-- And for .Net Core -->
    <!--  For compile time -->
    <file target="ref/netcoreapp" src="obj/SharpSvn.dll" />
    <file target="ref/netcoreapp" src="../src/SharpSvn/bin/win32/releasecore/SharpSvn.xml" />

    <!-- For runtime -->
    <file target="runtimes/win-x86/lib/netcoreapp" src="../src/SharpSvn/bin/win32/releasecore/SharpSvn.dll" />
    <file target="runtimes/win-x86/lib/netcoreapp" src="../src/SharpSvn/bin/win32/releasecore/ijwhost.dll" />
    <file target="runtimes/win-x86/lib/netcoreapp" src="../src/SharpSvn/bin/win32/releasecore/SharpSvn.pdb" />
    <file target="runtimes/win-x86/lib/netcoreapp" src="../src/SharpSvn/bin/win32/releasecore/SharpSvn.xml" />
    <file target="runtimes/win-x86/lib/netcoreapp" src="../src/SharpSvn/bin/win32/releasecore/SharpPlink-Win32.svnExe" />
    <file target="runtimes/win-x86/lib/netcoreapp" src="../imports/release/bin/SharpSvn-DB44-20-win32.svnDll" />

    <file target="runtimes/win-x64/lib/netcoreapp" src="../src/SharpSvn/bin/x64/releasecore/SharpSvn.dll" />
    <file target="runtimes/win-x64/lib/netcoreapp" src="../src/SharpSvn/bin/x64/releasecore/ijwhost.dll" />
    <file target="runtimes/win-x64/lib/netcoreapp" src="../src/SharpSvn/bin/x64/releasecore/SharpSvn.pdb" />
    <file target="runtimes/win-x64/lib/netcoreapp" src="../src/SharpSvn/bin/x64/releasecore/SharpSvn.xml" />
    <file target="runtimes/win-x64/lib/netcoreapp" src="../src/SharpSvn/bin/x64/releasecore/SharpPlink-x64.svnExe" />
    <file target="runtimes/win-x64/lib/netcoreapp" src="../imports/release/bin/SharpSvn-DB44-20-x64.svnDll" />

    <file target="runtimes/win-arm64/lib/netcoreapp" src="../src/SharpSvn/bin/arm64/releasecore/SharpSvn.dll" />
    <file target="runtimes/win-arm64/lib/netcoreapp" src="../src/SharpSvn/bin/arm64/releasecore/ijwhost.dll" />
    <file target="runtimes/win-arm64/lib/netcoreapp" src="../src/SharpSvn/bin/arm64/releasecore/SharpSvn.pdb" />
    <file target="runtimes/win-arm64/lib/netcoreapp" src="../src/SharpSvn/bin/arm64/releasecore/SharpSvn.xml" />
    <file target="runtimes/win-arm64/lib/netcoreapp" src="../src/SharpSvn/bin/arm64/releasecore/SharpPlink-ARM64.svnExe" />
    <file target="runtimes/win-arm64/lib/netcoreapp" src="../imports/release/bin/SharpSvn-DB44-20-ARM64.svnDll" />

    <!-- Automatically copy .svnExe/.svnDll files with SharpSvn during build -->
    <file target="build" src="builds/SharpSvn.targets" />
    <file target="buildTransitive" src="builds/SharpSvn.targets" />
  </files>
</package>
